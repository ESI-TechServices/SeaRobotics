  <DataSets>
    <DataSet Name="tmpCustomerBalance">
      <Query>
        <DataSourceName>dsCustSt</DataSourceName>
        <CommandText>="SELECT T1.Company,T1.BalanceDue,T1.CurrentBalance,T1.FutureBalance,T1.Over120,T1.Over30,T1.Over60,T1.Over90,T1.CurrencyCode,T1.CustNum
 FROM tmpCustomerBalance_" + Parameters!TableGuid.Value + " T1" + " WHERE T1.Company = '" + cstr(Parameters!Company.Value) + "' "  + " AND T1.CustNum = '" + 
cstr(Parameters!CustNum.Value) + "' "  + " AND T1.CurrencyCode = '" + cstr(Parameters!CurrencyCode.Value) + "' "</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
